    "visual_enhancers": {
      "shadows": "shadow-[0_8px_30px_rgb(0,0,0,0.04)]",
      "borders": "border border-[#6B8CAE]/20",
      "glass": "bg-white/60 backdrop-blur-md border border-white/40",
