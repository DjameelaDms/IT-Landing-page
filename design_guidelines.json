  "tech_stack": {
    "framework": "React (Create React App)",
    "styling": "Tailwind CSS",
    "ui_library": "Shadcn UI (Customized)",
    "motion": "Framer Motion (for entrance animations)",
    "icons": "lucide-react"
  },
  "instructions_to_main_agent": [
    "Install dependencies: npm install lucide-react framer-motion clsx tailwind-merge",
    "Configure Tailwind to use the custom font families (Playfair Display, Montserrat, Work Sans). Add Google Fonts import in index.css.",
    "Do NOT use default shadcn styles. Override border radius to be minimal/sharp for a professional look.",
    "Implement the 'Bento Grid' for solutions with CSS Grid, ensuring responsive layout (1 col mobile, 3 cols desktop).",
    "Use the provided colors strictly. Do not introduce new colors.",
    "Ensure the 'Coming Soon' badge is visible on all solution cards.",
    "Use the 'telemedicine' image for the 'Tele-Intubation' and 'Tele Code Blue' feature highlight if needed."
  ],
  "universal_guidelines": [
    "Dark colors look good when used independently without gradients. DO NOT use dark colors as gradients",
    "Don't make generic centered layouts, simplistic gradients, and uniform styling.",
    "Create depth through layered design elements with z-index hierarchy",
    "Use glass-morphism effects with backdrop filters (12-24px blur)",
    "You MUST NOT apply universal transition. Eg: transition: all.",
    "You MUST NOT center align the app container, ie do not add .App { text-align: center; }",
    "Always use modern button styles like pill-shaped, or sharp button with interaction animations"
  ]
}